<% provide(:title, 'Sign Up') %>
<% provide(:button_text, "Create My Account") %>
<h1>Sign Up</h1>
<div class = "row">
	<% if current_user %>
		<% if current_user.admin? %>
			<div class = "col-md-6 col-md-offset-3">
				<%= render 'formAdmin' %>
			</div>
		<% else %>
			<div class = "col-md-6 col-md-offset-3">
				<%= render 'form' %>
			</div>
		<% end %>
	<% else %>
		<div class = "col-md-6 col-md-offset-3">
			<%= render 'form' %>
		</div>
	<% end %>
</div>